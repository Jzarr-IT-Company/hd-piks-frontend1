{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "0.0.0" // Automatically picks the version you have installed
        }
    },
    "rules": {
        "no-unused-vars": "warn",
        "react/prop-types": "off"
    }
}